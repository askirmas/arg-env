{
  "name": "arg-env",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "setup": "./setup.sh",
    "dev": "npm run jest -- --watch --coverage=false",
    "build": "npm run compile",
    "prebuild": "npm run test",
    "postbuild": "npm run e2e",
    "compile": "tsc --project tsconfig.build.json",
    "precompile": "exit 0; rm -rf dist types",
    "postcompile": "./postcompile.sh",
    "e2e": "cd __e2e__ && npm run test",
    "test": "npm run jest",
    "pretest": "jest --clearCache && cd src/specs && ./get.sh",
    "jest": "jest --config=jest.config.json --runInBand",
    "prejest": "npm run precompile",
    "release": "np",
    "research": "npm run research/syntax && npm run research/propagation && npm run research/override",
    "research/syntax": "cd ./research/syntax && rm -rf output && ts-node-script ./index.ts && ./run.sh && ./output_md-start.sh && ts-node-script ./output.ts",
    "research/propagation": "rm -rf ./research/propagation/output && ts-node-script ./research/propagation/index.ts && cd ./research/propagation/output && cp ../template/docker/get.js docker/get.js && bash run.sh",
    "research/override": "rm -rf ./research/override/output && ts-node-script ./research/override/index.ts && cd ./research/override/output && cp ../template/docker/get.js docker/get.js && bash run.sh"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/askirmas/arg-env/labels/bug"
  },
  "author": "Andrii Kirmas <kirmas@phystech.edu>",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-extended": "^2.9.0",
    "envfile": "^6.17.0",
    "git-hooks-wrapper": "^0.7.1",
    "globby": "^11.0.4",
    "jest": "^27.1.1",
    "np": "^7.5.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "np": {
    "yarn": false,
    "branch": "main",
    "2fa": false
  },
  "files": [
    "dist",
    "types"
  ],
  "types": "types",
  "main": "dist/index.js"
}
