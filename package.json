{
  "name": "arg-env",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "setup": "./setup.sh",
    "dev": "npm run jest -- --watch --coverage=false",
    "build": "npm run compile",
    "prebuild": "./prebuild.sh",
    "postbuild": "npm run e2e",
    "compile": "tsc --project tsconfig.build.json",
    "precompile": "exit 0; rm -rf dist types",
    "postcompile": "./postcompile.sh",
    "e2e": "cd __e2e__ && npm run test",
    "test": "npm run jest",
    "pretest": "jest --clearCache",
    "jest": "jest --config=jest.config.json --runInBand",
    "prejest": "npm run precompile",
    "output": "ts-node-script output.ts",
    "release": "np"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/askirmas/env_file/labels/bug"
  },
  "author": "Andrii Kirmas <kirmas@phystech.edu>",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-extended": "^2.9.0",
    "git-hooks-wrapper": "^0.7.1",
    "jest": "^27.1.1",
    "np": "^7.5.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "np": {
    "yarn": false,
    "branch": "main",
    "2fa": false
  },
  "files": [
    "dist",
    "types"
  ],
  "types": "types",
  "main": "dist/index.js"
}
